{
  "name": "video-template",
  "version": "1.1.64",
  "description": "Template model describing video structure",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "directories": {
    "dist": "dist"
  },
  "scripts": {
    "convert": "node tools/convert.js",
    "compile": "node tools/convert.js",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "release": "npm run build && npm publish"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "video",
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Vinlic/video-template"
  },
  "author": "Vinlic",
  "license": "ISC",
  "dependencies": {
    "aggregation-ssml": "^1.0.73",
    "date-fns": "^2.28.0",
    "fast-xml-parser": "^4.0.7",
    "goodid.js": "^1.0.8",
    "lodash": "^4.17.21",
    "uuid": "^8.3.2",
    "xmlbuilder2": "^3.0.2"
  },
  "engines": {
    "node": ">=12.22.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.26",
    "@types/uuid": "^8.3.4",
    "axios": "^0.27.2",
    "tsup": "^5.12.6",
    "typescript": "^4.6.3"
  }
}
